{
  "name": "Skatespot.io",
  "version": "1.0.0",
  "engines": {
    "node": "8.11.4",
    "npm": "6.4.0"
  },
  "description": "A Simple Login Example using Node.js, Mongo and Redis",
  "main": "./server/app.js",
  "scripts": {
    "buildAppBundle": "webpack --config webpack.prod.js --mode=production --optimize-minimize --optimize-occurrence-order",
    "watchAppBundle": "webpack-dev-server --config webpack.watch.js --hot --mode=development",
    "build": "npm run buildAppBundle",
    "postinstall": "npm run buildAppBundle",
    "nodemon": "nodemon --watch ./server ./server/app.js",
    "start": "node ./server/app.js",
    "dev": "start npm run watchAppBundle && start npm run nodemon",
    "pretest": "eslint ./server",
    "test": "echo \"Tests complete\""
  },
  "keywords": [
    "Node",
    "MVC",
    "Redis",
    "Login"
  ],
  "author": "Luke Lapresi",
  "license": "Apache 2.0",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^3.4.2",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "helmet": "^1.3.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^5.5.0",
    "leaflet": "^1.6.0",
    "mongoose": "^4.13.19",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-leaflet": "^2.6.0",
    "react-leaflet-div-icon": "^1.1.0",
    "react-responsive": "^4.1.0",
    "react-router-dom": "^4.3.1",
    "react-router-prop-types": "^1.0.4",
    "react-star-rating-component": "^1.4.1",
    "react-transition-group": "^2.9.0",
    "rmwc": "^2.2.3",
    "serve-favicon": "^2.5.0",
    "underscore": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "mini-css-extract-plugin": "^0.4.5",
    "nodemon": "^1.19.4",
    "style-loader": "^0.23.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.9.0",
    "workbox-webpack-plugin": "^3.6.1"
  }
}
