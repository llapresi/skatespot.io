{
  "name": "Logins",
  "version": "1.0.0",
  "engines": {
    "node": "7.5.0",
    "npm": "4.0.5"
  },
  "description": "A Simple Login Example using Node.js, Mongo and Redis",
  "main": "./server/app.js",
  "scripts": {
    "buildLoginBundle": "webpack --config login.config.js --mode production",
    "buildAppBundle": "webpack --mode production",
    "build": "npm run buildLoginBundle && npm run buildAppBundle",
    "watchAppBundle": "webpack-dev-server --hot --mode development",
    "watchLoginBundle": "webpack --config login.config.js --watch --mode development --info-verbosity verbose",
    "nodemon": "nodemon --watch ./server ./server/app.js",
    "start": "node ./server/app.js",
    "dev": "start npm run watchAppBundle && start npm run nodemon",
    "pretest": "eslint ./server --fix",
    "test": "echo \"Tests complete\""
  },
  "keywords": [
    "Node",
    "MVC",
    "Redis",
    "Login"
  ],
  "author": "Cody Van De Mark",
  "license": "Apache 2.0",
  "dependencies": {
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "connect-redis": "^3.0.2",
    "cookie-parser": "^1.3.3",
    "csurf": "^1.8.3",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.13.0",
    "helmet": "^1.3.0",
    "mongoose": "^4.4.6",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-responsive": "^4.1.0",
    "react-transition-group": "^2.3.1",
    "serve-favicon": "^2.5.0",
    "underscore": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.1",
    "google-map-react": "^0.34.0",
    "nodemon": "^1.17.3",
    "react-hot-loader": "^4.1.1",
    "rmwc": "^1.5.4",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
}
